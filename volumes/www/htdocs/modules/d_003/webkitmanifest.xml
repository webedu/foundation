    <url>http://www.webgeo.de</url>
    <license_url>http://www.webgeo.de/impressum</license_url>
    <licence_logo>blank.gif</licence_logo>
    <!--******Glossar******-->
    <wordreference label="" uri="" form="" />
    <!--******Sitemap******-->
    <sitemap></sitemap>
    <!--******Navigationsverhalten******-->
    <navigation continuous="true" menus="true" menusvisible="true" sectionsrestricted="false" autoCurrentPageMarker="true" skipMenusAndUseProgressBar="false" />
    <!--******Zugriffsgeschützte Module******-->
    <accesscontrol authentication="false" registration="false" registrationpage="materials/registration.html" />
    <!--******User-Logging******-->
    <userlogging keepstatus="true" usedb="false" httpmethod="POST" />
    <!--******Schnittstelle zu serverseitigen Skripts zur DB-Anbindung******-->
    <phpinterface>php/interfacetowebkit.php</phpinterface>
    <!--******SCORM-Einstellungen******-->
    <beSCO>true</beSCO>
    <!--******Success Tracking******-->
    <success_tracking>use_score</success_tracking>
    <total_score>20</total_score>
    <scaled_passing_score>0.6</scaled_passing_score>
    <!--Developer-Vars-->
    <webkitdevelopment debugging="false" />
  </metadata>
  <!--************************METADATA end************************-->
  <!--************************ORGANIZATION begin************************-->
